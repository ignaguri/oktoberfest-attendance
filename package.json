{
  "name": "oktoberfest-attendance",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --port 3008",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "sup:db:pull": "supabase db pull",
    "sup:db:push": "supabase db push",
    "sup:db:reset": "supabase db reset",
    "sup:db:types": "supabase gen types typescript --local > lib/database.types.ts",
    "sup:mig:new": "supabase migration new",
    "sup:start": "supabase start",
    "sup:stop": "supabase stop",
    "sup:restart": "supabase stop && supabase start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "version:show": "tsx scripts/version.ts show",
    "version:patch": "tsx scripts/version.ts patch",
    "version:minor": "tsx scripts/version.ts minor",
    "version:major": "tsx scripts/version.ts major",
    "version:set": "tsx scripts/version.ts set",
    "release:patch": "pnpm run version:patch && git add . && git commit -m \"chore: bump version\" && git tag v$(pnpm exec tsx scripts/version.ts show) && git push && git push --tags",
    "release:minor": "pnpm run version:minor && git add . && git commit -m \"chore: bump version\" && git tag v$(pnpm exec tsx scripts/version.ts show) && git push && git push --tags",
    "release:major": "pnpm run version:major && git add . && git commit -m \"chore: bump version\" && git tag v$(pnpm exec tsx scripts/version.ts show) && git push && git push --tags",
    "prepare": "husky",
    "novu:sync": "tsx scripts/novu-sync.ts",
    "novu:local": "npx novu@latest dev -p 3008 -r /api/novu",
    "cron:trigger": "tsx scripts/trigger-cron.ts",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:install-browsers": "playwright install --with-deps"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@hookform/resolvers": "^5.2.2",
    "@next/third-parties": "^15.5.3",
    "@novu/api": "^1.6.0",
    "@novu/nextjs": "^3.9.3",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@sentry/nextjs": "^10.12.0",
    "@serwist/next": "^9.2.1",
    "@supabase/auth-ui-react": "0.4.7",
    "@supabase/auth-ui-shared": "0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "2.57.4",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/node": "24.5.2",
    "@types/qrcode": "^1.5.5",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "dotenv": "^17.2.2",
    "encoding": "0.1.13",
    "firebase": "^12.2.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-themes": "^0.4.6",
    "next-view-transitions": "^0.3.4",
    "qrcode": "^1.5.4",
    "radix-ui": "latest",
    "react": "19.1.1",
    "react-datepicker": "8.7.0",
    "react-day-picker": "^9.10.0",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "serwist": "^9.2.1",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "vaul": "^1.1.2",
    "zod": "^4.1.9",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@next/eslint-plugin-next": "15.5.3",
    "@novu/framework": "^2.7.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "9.35.0",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.2.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.0.0",
    "import-in-the-middle": "^1.14.2",
    "jsdom": "^27.0.0",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "require-in-the-middle": "^7.5.2",
    "supabase": "2.40.7",
    "tailwindcss": "4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "eslint": "$eslint"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@sentry/cli",
      "@tailwindcss/oxide",
      "@vercel/speed-insights",
      "esbuild",
      "protobufjs",
      "sharp",
      "supabase",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.14.0"
}

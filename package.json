{
  "name": "oktoberfest-attendance",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --port 3008",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "sup:db:pull": "supabase db pull",
    "sup:db:push": "supabase db push",
    "sup:db:reset": "supabase db reset",
    "sup:db:types": "supabase gen types typescript --local > lib/database.types.ts",
    "sup:mig:new": "supabase migration new",
    "sup:start": "supabase start",
    "sup:stop": "supabase stop",
    "sup:restart": "supabase stop && supabase start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "version:show": "tsx scripts/version.ts show",
    "version:patch": "tsx scripts/version.ts patch",
    "version:minor": "tsx scripts/version.ts minor",
    "version:major": "tsx scripts/version.ts major",
    "version:set": "tsx scripts/version.ts set",
    "release:patch": "pnpm run version:patch && git add . && git commit -m \"chore: bump version\" && git tag v$(pnpm exec tsx scripts/version.ts show) && git push && git push --tags",
    "release:minor": "pnpm run version:minor && git add . && git commit -m \"chore: bump version\" && git tag v$(pnpm exec tsx scripts/version.ts show) && git push && git push --tags",
    "release:major": "pnpm run version:major && git add . && git commit -m \"chore: bump version\" && git tag v$(pnpm exec tsx scripts/version.ts show) && git push && git push --tags",
    "prepare": "husky",
    "novu:sync": "tsx scripts/novu-sync.ts",
    "novu:local": "npx novu@latest dev -p 3008 -r /api/novu",
    "cron:trigger": "tsx scripts/trigger-cron.ts",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:install-browsers": "playwright install --with-deps"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@hookform/resolvers": "^5.2.2",
    "@next/third-parties": "^16.1.1",
    "@novu/api": "^3.11.2",
    "@novu/nextjs": "^3.11.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@sentry/nextjs": "^10.32.1",
    "@serwist/next": "^9.4.2",
    "@supabase/auth-ui-react": "0.4.7",
    "@supabase/auth-ui-shared": "0.1.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "2.89.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/node": "25.0.3",
    "@types/qrcode": "^1.5.6",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "classnames": "2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "dotenv": "^17.2.3",
    "encoding": "0.1.13",
    "firebase": "^12.7.0",
    "framer-motion": "^12.23.26",
    "html-to-image": "^1.11.13",
    "isomorphic-dompurify": "^2.35.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "next-view-transitions": "^0.3.5",
    "qrcode": "^1.5.4",
    "radix-ui": "latest",
    "react": "19.2.3",
    "react-confetti-explosion": "^3.0.3",
    "react-datepicker": "9.1.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "recharts": "^3.6.0",
    "serwist": "^9.4.2",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "swiper": "^12.0.3",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "vaul": "^1.1.2",
    "zod": "^4.2.1",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "16.1.1",
    "@novu/framework": "^2.9.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/forms": "0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.3.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "import-in-the-middle": "^2.0.1",
    "jsdom": "^27.4.0",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "require-in-the-middle": "^8.0.1",
    "supabase": "2.70.5",
    "tailwindcss": "4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.50.1",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "eslint": "$eslint",
    "html-encoding-sniffer": {
      "@exodus/bytes": "npm:encoding@0.1.13"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@sentry/cli",
      "@tailwindcss/oxide",
      "@vercel/speed-insights",
      "esbuild",
      "protobufjs",
      "sharp",
      "supabase",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.14.0"
}
